#import "ChooseNumMessage.h"

@implementation ChooseNumMessage

-(void)dealloc
{
    [super dealloc];
}

- (NSString *)getRequest
{
    NSDictionary *headerData = [[NSDictionary alloc] initWithObjectsAndKeys:CHOOSENUMBER_BIZCODE,@"bizCode",nil];
    //    NSString* BundleVersion = [[NSBundle mainBundle] objectForInfoDictionaryKey:(NSString *)kCFBundleVersionKey];
    
    NSString *requestClass = [[NSString alloc] initWithFormat:JSON_BODY_REUEST,[CHOOSENUMBER_BIZCODE uppercaseString]];
    
    NSDictionary *bodyData = [[NSDictionary alloc] initWithObjectsAndKeys:
                              requestClass,@"@class",
                              [requestInfo objectForKey:@"expand"]==nil?[NSNull null]:[requestInfo objectForKey:@"expand"],@"expand",
                              [requestInfo objectForKey:@"phoneNum"],@"phoneNum",
                              [requestInfo objectForKey:@"pageIndex"],@"pageIndex",
                              [requestInfo objectForKey:@"pageCount"],@"pageCount",
                              [requestInfo objectForKey:@"cardType"],@"cardType",
                              [requestInfo objectForKey:@"numType"],@"numType",
                              [requestInfo objectForKey:@"areaId"],@"areaId",
                              [requestInfo objectForKey:@"productId"],@"productId",nil];
    //                              [requestInfo objectForKey:@"phoneNums"],@"phoneNums",
    //                              [requestInfo objectForKey:@"phoneNumPrice"],@"phoneNumPrice",nil];
    
    [requestClass release];
    
    NSString *requestStr = [self getRequestJSONFromHeader:headerData
                                                 withBody:bodyData];
    [headerData release];
    [bodyData release];
    
    NSLog(@"检查版本报文:%@",requestStr);
    return requestStr;//[message hexStringDes:requestStr withEncrypt:YES];
}

